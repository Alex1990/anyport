{
  "name": "anyport",
  "version": "0.1.0",
  "description": "A command line tool to get the available ports",
  "main": "lib/index.js",
  "bin": {
    "anyport": "./bin/anyport.js"
  },
  "scripts": {
    "compile": "npm run test && rimraf lib && babel src -d lib",
    "cover": "istanbul cover _mocha -- --compilers js:babel-core/register -R spec",
    "coveralls": "npm run cover && cat coverage/lcov.info | coveralls",
    "lint": "eslint .",
    "test": "npm run lint && mocha --compilers js:babel-core/register",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Alex1990/anyport.git"
  },
  "keywords": [
    "port"
  ],
  "author": "Alex Chao",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.11",
    "eslint": "^3.7.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.16.0",
    "istanbul": "^1.1.0-alpha.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "async": "^2.1.4",
    "commander": "^2.9.0",
    "detect-port": "^1.0.7"
  },
  "pre-commit": [
    "lint"
  ]
}
